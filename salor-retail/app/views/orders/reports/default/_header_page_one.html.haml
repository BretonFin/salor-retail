.header-page-one
  - if Vendor.find_by_id(@current_vendor.id).logo_image
    %img.vendorlogo.floatright{ :src => "/vendors/#{ @current_vendor.id }/logo_invoice" }
  
  .clear
  
  .company_data.floatright= raw Kramdown::Document.new(@current_vendor.salor_configuration.invoice_blurb).to_html
      
  .clear